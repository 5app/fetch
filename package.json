{
	"name": "@5app/fetch",
	"version": "1.0.3",
	"description": "Description",
	"main": "dist/index.js",
	"type": "module",
	"files": [
		"dist/*"
	],
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsc",
		"prepare": "is-ci || husky install",
		"lint": "eslint --ignore-path .gitignore ./",
		"lint-fix": "npm run lint -- --fix",
		"lint-diff": "LIST=`git diff-index --name-only HEAD | grep '.*\\.js$';`; if [ \"$LIST\" ]; then eslint $LIST; fi",
		"precommit-msg": "echo 'Running pre-commit checks... (skip using --no-verify)'",
		"spec": "npm run build",
		"test": "npm run lint && npm run spec"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/5app/fetch.git"
	},
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/5app/fetch/issues"
	},
	"homepage": "https://github.com/5app/fetch#readme",
	"devDependencies": {
		"@5app/prettier-config": "^1.0.4",
		"@5app/semantic-release-config": "^1.0.1",
		"@commitlint/cli": "^17.1.2",
		"@commitlint/config-conventional": "^17.1.0",
		"@tsconfig/node16-strictest-esm": "^1.0.3",
		"@typescript-eslint/eslint-plugin": "^5.36.2",
		"@typescript-eslint/parser": "^5.36.2",
		"eslint": "^8.23.0",
		"eslint-config-5app": "^0.16.4",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.1",
		"is-ci": "^3.0.1",
		"prettier": "^2.7.1",
		"pretty-quick": "^3.1.3",
		"semantic-release": "^19.0.5",
		"typescript": "^4.8.2"
	},
	"publishConfig": {
		"access": "public"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		],
		"rules": {
			"subject-case": [
				2,
				"never",
				[
					"start-case",
					"pascal-case"
				]
			]
		}
	},
	"release": {
		"extends": "@5app/semantic-release-config"
	},
	"prettier": "@5app/prettier-config",
	"dependencies": {
		"node-fetch": "^3.2.10"
	}
}
